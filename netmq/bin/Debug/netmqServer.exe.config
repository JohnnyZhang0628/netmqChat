<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--推送消息的ip和端口-->
    <add key="pubIP" value="127.0.0.1"/>
    <add key="pubPort" value="1234"/>
    <!--接受消息和端口-->
    <add key="serverIP" value="127.0.0.1"/>
    <add key="serverPort" value="5678"/>
  </appSettings>
</configuration>